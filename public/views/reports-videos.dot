			function parseVideoReportsArchive(reports) {
				const data = [];
				
				for (const report of reports) {
					const archiveId = report.archive_id;
                    const reportId = report.report_id;
					const timestamp = report.timestamp;
					const videoTimestamp = report.video_timestamp;
					const videoId = report.video_id;
					const email = report.email;
					const type = report.type;
					const message = report.message;
					
					const timestampFormatted = createDateTimestamp(timestamp);
					const videoTimestampFormatted = createDateTimestamp(videoTimestamp);

					const deleteVideoButton = '<button class="btn btn-icon btn-2 btn-primary me-1" onclick="deleteVideo2(this)" data-videoid="' + videoId + '" data-bs-toggle="tooltip" data-bs-placement="top" data-container="body" data-animation="true" aria-label="delete video" data-bs-original-title="delete video"><span class="btn-inner--icon"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-lg" viewBox="0 0 16 16"><path d="M2.146 2.854a.5.5 0 1 1 .708-.708L8 7.293l5.146-5.147a.5.5 0 0 1 .708.708L8.707 8l5.147 5.146a.5.5 0 0 1-.708.708L8 8.707l-5.146 5.147a.5.5 0 0 1-.708-.708L7.293 8 2.146 2.854Z"/></svg></span></button>';
					const deleteArchiveButton = '<button class="btn btn-icon btn-2 btn-primary" onclick="deleteArchive(this)" data-archiveid="' + archiveId + '" data-bs-toggle="tooltip" data-bs-placement="top" data-container="body" data-animation="true" aria-label="delete archive" data-bs-original-title="delete archive"><span class="btn-inner--icon"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16"><path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5Zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5Zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6Z"/><path d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1ZM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118ZM2.5 3h11V2h-11v1Z"/></svg></span></button>';

					data.push([
						deleteVideoButton + deleteArchiveButton, 
						timestampFormatted,
						videoTimestampFormatted,
						archiveId.toString(),
						reportId.toString(),
						'<a href="' + NODE_BASE_URL + '/watch?v=' + videoId + '" target="_blank">' + videoId + '</a>',
						email,
						type,
						message
					]);
				}

				let tableData = {
					headings: [ "actions", "report date", 'video date', "archive id", "report id", "video id", "email", "type", "message" ],
					data: data
				};
				
				return tableData;
			}

			function populateVideoReportsTable(parsedVideoReports) {
				if(DATA_TABLE_VIDEO_REPORTS != null) {
					DATA_TABLE_VIDEO_REPORTS.destroy();
				}
				
				DATA_TABLE_VIDEO_REPORTS = new simpleDatatables.DataTable(
					"#video-reports-table",
					{
						searchable: true,
						fixedHeight: true,
					}
				);
				
				DATA_TABLE_VIDEO_REPORTS.insert(parsedVideoReports);
			}
			
			function populateVideoReportsArchiveTable(parsedVideoReportArchives) {
				if(DATA_TABLE_VIDEO_REPORTS_ARCHIVE != null) {
					DATA_TABLE_VIDEO_REPORTS_ARCHIVE.destroy();
				}

				DATA_TABLE_VIDEO_REPORTS_ARCHIVE = new simpleDatatables.DataTable(
					"#video-reports-archive-table",
					{
						searchable: true,
						fixedHeight: true,
					}
				);
				
				DATA_TABLE_VIDEO_REPORTS_ARCHIVE.insert(parsedVideoReportArchives);
			}
			
			function archiveReport(button) {
				const reportId = $(button).data('reportid');
				
				$.ajax({type: 'POST', url: '/reports/videos/archive', data: {reportId: reportId}})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						const row = button.closest('tr');

						DATA_TABLE_VIDEO_REPORTS.rows().remove(parseInt(row.dataIndex));
						
						getVideoReportsArchive();
					}
				})
				.fail(function()
				{
					console.log('unable to communicate with node');
				});
			}
			
			function deleteReport(button) {
				const reportId = $(button).data('reportid');
				
				$.ajax({type: 'POST', url: '/reports/videos/delete', data: {reportId: reportId}})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						const row = button.closest('tr');

						DATA_TABLE_VIDEO_REPORTS.rows().remove(parseInt(row.dataIndex));
					}
				})
				.fail(function()
				{
					console.log('unable to communicate with node');
				});
			}
			
			function deleteArchive(button) {
				const archiveId = $(button).data('archiveid');
				
				$.ajax({type: 'POST', url: '/reports/videos/archive/delete', data: {archiveId: archiveId}})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						const row = button.closest('tr');

						DATA_TABLE_VIDEO_REPORTS_ARCHIVE.rows().remove(parseInt(row.dataIndex));
					}
				})
				.fail(function() {
					console.log('unable to communicate with node');
				});
			}
			
			function deleteVideo1(button) {
				const videoId = $(button).data('videoid');
				
				const videoIds = [videoId];
				
				$.ajax({type: 'POST', url: '/videos/delete', data: JSON.stringify({ videoIds: videoIds }), contentType: 'application/json; charset=utf-8'})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						showAlert(false, 'video deleted');
					}
				})
				.fail(function()
				{
					console.log('unable to communicate with node');
				});
			}
			
			function deleteVideo2(button) {
				const videoId = $(button).data('videoid');

				const videoIds = [videoId];
				
				$.ajax({type: 'POST', url: '/videos/delete', data: JSON.stringify({ videoIds: videoIds }), contentType: 'application/json; charset=utf-8'})
				.done(function (data, textStatus, xhr)
				{
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						showAlert(false, 'video deleted');
					}
				})
				.fail(function()
				{
					console.log('unable to communicate with node');
				});
			}
			
			function refreshHoverTooltips() {
				$(function () {
					$('[data-bs-toggle="tooltip"]').tooltip({
						trigger : 'hover'
					});

					$('[data-bs-toggle="tooltip"]').on('click', function () {
						$(this).tooltip('hide')
					});
				});
			}
			
			function showAlert(isError, message) {
				if(isError) {
					Swal.fire({
						icon: 'warning',
						html: message,
						confirmButtonColor: '#3085d6',
						confirmButtonText: 'Ok'
					});
				}
				else {
					Swal.fire({
						icon: 'success',
						html: message,
						showConfirmButton: false,
						timer: 1500
					})
				}
			}
			
			function createDateTimestamp(timestamp)	{
				let date = new Date(timestamp);
				let weekday = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
				
				return ('0' + (date.getMonth() + 1).toString()).substr(-2) + '/' + ('0' + date.getDate().toString()).substr(-2)  + '/' + (date.getFullYear().toString()).substr(2) + '(' + weekday[date.getDay()] + ')' + date.toTimeString().split(' ')[0];
			}
		</script>
	</body>
