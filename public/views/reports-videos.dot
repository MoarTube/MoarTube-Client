		<script>
			let NODE_BASE_URL;
			
			let DATA_TABLE_VIDEO_REPORTS;
			let DATA_TABLE_VIDEO_REPORTS_ARCHIVE;
			
			$(document).ready(async function() {
				await getNodeInformation();
				await setContentChecked('videoReports');
				await getNewContentCounts();
				await getVideoReports();
				await getVideoReportsArchive();
			});
			
			function getNodeInformation() {
				$.ajax({type: 'GET', url: '/settings/node'})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						const nodeSettings = data.nodeSettings;

						const nodeProtocol = nodeSettings.publicNodeProtocol;
						const nodeAddress = nodeSettings.publicNodeAddress;
						const nodePort = nodeSettings.publicNodePort;
						
						NODE_BASE_URL = nodeProtocol + '://' + nodeAddress + ':' + nodePort;
						
						$('#node-link').attr('href', NODE_BASE_URL);
					}
				})
				.fail(function()
				{
					console.log('unable to communicate with node');
				});
			}

			function setContentChecked(contentType) {
				return new Promise(function(resolve, reject) {
					$.ajax({type: 'POST', url: '/node/contentChecked', data: { contentType: contentType }})
					.done(function (data, textStatus, xhr) {
						if(data.isError) {
							showAlert(true, data.message);

							reject();
						}
						else {
							resolve();
						}
					})
					.fail(function()
					{
						console.log('unable to communicate with node');

						reject();
					});
				});
			}

			function getNewContentCounts() {
				return new Promise(function(resolve, reject) {
					$.ajax({type: 'GET', url: '/node/newContentCounts'})
					.done(function (data, textStatus, xhr)
					{
						if(data.isError) {
							reject();
						}
						else {
							const newContentCounts = data.newContentCounts;

							const newCommentsCount = newContentCounts.newCommentsCount;
							const newVideoReportsCount = newContentCounts.newVideoReportsCount;
							const newCommentReportsCount = newContentCounts.newCommentReportsCount;

							const newReportsCount = newVideoReportsCount + newCommentReportsCount;

							if(newCommentsCount > 0) {
								$('#new-comments-count').text(newCommentsCount);
							}

							if(newReportsCount > 0) {
								$('#new-reports-count').text(newReportsCount);
							}
							
							if(newVideoReportsCount > 0) {
								$('#new-video-reports-count').text(newVideoReportsCount);
							}
							
							if(newCommentReportsCount > 0) {
								$('#new-comment-reports-count').text(newCommentReportsCount);
							}
							
							resolve();
						}
					})
					.fail(function()
					{
						console.log('unable to communicate with node');
						
						reject();
					});
				});
			}
		
			function getVideoReports() {
				$.ajax({type: 'GET', url: '/reports/videos/all'})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						const reports = data.reports;
						
						const parsedReports = parseVideoReports(reports);
						
						populateVideoReportsTable(parsedReports);
						
						refreshHoverTooltips();
					}
				})
				.fail(function() {
					console.log('unable to communicate with node');
				});
			}
			
			function getVideoReportsArchive() {
				$.ajax({type: 'GET', url: '/reports/videos/archive/all'})
				.done(function (data, textStatus, xhr) {
					if(data.isError) {
						showAlert(true, data.message);
					}
					else {
						const reports = data.reports;
						
						const parsedReports = parseVideoReportsArchive(reports);

						populateVideoReportsArchiveTable(parsedReports);
						
						refreshHoverTooltips();
					}
				})
				.fail(function() {
					console.log('unable to communicate with node');
				});
			}

			function parseVideoReports(reports) {
				const data = [];
				
				for (const report of reports) {
					const reportId = report.report_id;
                    const timestamp = report.timestamp;
					const videoTimestamp = report.video_timestamp;
                    const videoId = report.video_id;
					const email = report.email;
					const type = report.type;
					const message = report.message;
					
					const timestampFormatted = createDateTimestamp(timestamp);
					const videoTimestampFormatted = createDateTimestamp(videoTimestamp);

